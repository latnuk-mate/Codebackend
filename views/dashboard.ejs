
    <table class="table table-stripped">
      <thead>
        <tr>
        <th>AuthorName</th>
        <th>createdAt</th>
        <th>createdBooks</th>
        </tr>
      </thead>
     <% Authors.forEach(author => { %>
        <tbody>
            <tr>
            <td><%= author.AuthorName %></td>
            <td><%= helper.formatDate(author.createdAt, "dddd, MMMM Do YYYY, h:mm:ss a") %></td>
            <td></td>
            </tr>
          </tbody>
     <% }) %>
    </table>